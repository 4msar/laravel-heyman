{
    "name": "imanghafoori/laravel-heyman",
    "description": "A Package which allows you to easily authorize your users",
    "keywords": ["laravel", "laravel-authorazation", "laravel5-package", "PHP", "laravel5"],
    "license": "MIT",
    "homepage": "https://github.com/imanghafoori1/laravel-HeyMan",
    "authors": [
        {
            "name": "Iman Ghafoori",
            "email": "imanghafoori1@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "imanghafoori/laravel-terminator" : "*",
        "laravel/framework":"~5.3"
    },
    "autoload-dev": {
        "classmap": ["tests/TestCase.php"]
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Imanghafoori\\HeyMan\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Imanghafoori\\HeyMan\\HeyManServiceProvider"
            ]
        }
    }
}
